server {    listen 443 ssl http2;    server_name mcp.official-digitech.com;    ssl_certificate     /etc/letsencrypt/live/mcp.official-digitech.com/fullchain.pem;    ssl_certificate_key /etc/letsencrypt/live/mcp.official-digitech.com/privkey.pem;    location / {        proxy_pass         http://127.0.0.1:8000;        proxy_set_header   Host $host;        proxy_set_header   X-Real-IP $remote_addr;        proxy_set_header   X-Forwarded-Proto $scheme;    }    ssl_protocols       TLSv1.2 TLSv1.3;    ssl_ciphers         HIGH:!aNULL:!MD5;}